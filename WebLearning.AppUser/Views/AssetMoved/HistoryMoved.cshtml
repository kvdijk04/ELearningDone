@using WebLearning.Contract.Dtos.Assets.Moved;

@model IEnumerable<AssetMovedHistoryDto>
@{
    Layout = null;
}
<table class="table table-striped table-bordered bulk_action" id="tabel">
    <p>Tổng cộng: @Model.Count()</p>
    <thead>
        <tr>
            <th>Id</th>
            <th>Ngày tạo</th>
            <th>#</th>
        </tr>
    </thead>
    <tbody class="bodyContainer">

        @if (Model != null && Model.Count() > 0)
        {
            @foreach (var item in Model)
            {
                <tr>
                    <td>@item.Id</td>
                   
                    <td>@item.DateCreated</td>
                    <td>
                        <div>
                            <a href="/in-tai-san-dieu-chuyen/@item.Id" class="btn btn-primary btn-xs"><i class="fa fa-folder"></i> Chi tiết </a>
                        </div>
                    </td>
                </tr>
            }

        }
        else
        {
            <tr><td valign="top" colspan="15" class="dataTables_empty">Không có dữ liệu</td></tr>

        }


    </tbody>
</table>